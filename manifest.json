{
  "name": "Cat Friend!",
  "description": "üêà‚Äç‚¨õ Source: https://github.com/hannahilea/cat-friend-chrome-extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "info.html",
    "default_icon": "images/tail1.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "web_accessible_resources": [
    {
    "matches": ["<all_urls>"],
    "resources": ["images/tail1.png", "images/tail2.png", "images/tail3.png", "images/tail4.png"]
  }
]
}
